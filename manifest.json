{
  "name": "Tab Title",
  "description": "Capture the current tab title",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "/images/incognito-16.png",
    "32": "/images/incognito-32.png",
    "48": "/images/incognito-48.png"
  },
  "permissions": ["activeTab", "tabs"],  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}